{
  "compilerOptions": {
    "target": "ES6",
    "module": "ESNext",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "baseUrl": "./",
    "lib": ["dom", "dom.iterable", "esnext"],
    "types": ["vite/client"]
  },
  "include": [
    "frontend/src",
    "frontend/src/vite-env.d.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
